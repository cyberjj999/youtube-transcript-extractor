{
    "name": "Youtube Transcript Extractor",
    "version": "1.0",
    "manifest_version": 2,
    "description": "Extract Youtube Transcript for Maximum Productivity",
    "icons": {
        "128": "icon128.png",
        "48": "icon48.png",
        "16": "icon16.png"
    },
    "browser_action": {
        "default_icon": "icon16.png"
    },
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },    
    "permissions": ["tabs", "<all_urls>"]
}
